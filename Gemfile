# A sample Gemfile
source "https://rubygems.org"
gem 'sinatra'

gem 'activesupport'

gem 'sinatra-contrib', group: :development

group :development, :test do
  gem 'awesome_print'
  gem 'racksh'
  gem 'debugger'
  gem 'pry'
  gem 'pry-debugger'
end
group :test do
  gem 'rack-test'
  gem 'minitest'
end

